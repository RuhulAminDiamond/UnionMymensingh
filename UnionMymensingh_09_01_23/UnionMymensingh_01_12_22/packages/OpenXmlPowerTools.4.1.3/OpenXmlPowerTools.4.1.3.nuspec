  <metadata>
    <id>OpenXmlPowerTools</id>
    <version>4.1.3</version>
    <title>OpenXml Power Tools</title>
    <authors>Eric White</authors>
    <owners>Eric White</owners>
    <licenseUrl>https://github.com/OfficeDev/Open-Xml-PowerTools/blob/vNext/LICENSE.txt</licenseUrl>
